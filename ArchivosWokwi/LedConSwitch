#Codigo micropython
print("Hello, ESP32!")

from machine import Pin
Switch = Pin (18, Pin.IN)
ledAzul = Pin (27, Pin.OUT)

while True:

  if (Switch.value()==1):
    ledAzul.value(1)
    print ("Siuuuuuuu")


  else: 
    ledAzul.value(0)
    print ("Nouuuuuuuuu")

